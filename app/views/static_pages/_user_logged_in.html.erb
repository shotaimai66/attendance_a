<% provide(:search_name, 'コンテンツ検索') %>
<% provide(:path, root_path) %>

  <div class="row">
      
      
    
    
	
    <table class="table table-bordered">
        <tr>
            <td data-sheets-value="{&quot;1&quot;:2,&quot;2&quot;:&quot;時間管理表&quot;}">
                <%= link_to "←", user_work_path(current_user,@back), class: "btn btn-default btn-lg active" %>
                2018年<%= @month %>月の時間管理表
                <%= link_to "→", user_work_path(current_user,@next), class: "btn btn-default btn-lg active" %></td>
            <td data-sheets-value="{&quot;1&quot;:2,&quot;2&quot;:&quot;指定勤務時間&quot;}">指定勤務時間  8.00</td>
            <td rowspan="1" colspan="3" data-sheets-value="{&quot;1&quot;:2,&quot;2&quot;:&quot;基本時間&quot;}">7.50  基本時間</td>
            <td data-sheets-value="{&quot;1&quot;:2,&quot;2&quot;:&quot;初日&quot;}">初日 <%= current_month_first %></td>
        </tr>
        <tr>
            <td data-sheets-value="{&quot;1&quot;:2,&quot;2&quot;:&quot;所属&quot;}">所属</td>
            <td data-sheets-value="{&quot;1&quot;:2,&quot;2&quot;:&quot;氏名&quot;}">氏名 <%= current_user.name %></td>
            <td data-sheets-value="{&quot;1&quot;:2,&quot;2&quot;:&quot;コード&quot;}">コード</td>
            <td></td>
            <td data-sheets-value="{&quot;1&quot;:2,&quot;2&quot;:&quot;出勤時間&quot;}">出勤時間 <%= current_user.works.count %>日</td>
            <td data-sheets-value="{&quot;1&quot;:2,&quot;2&quot;:&quot;締め&quot;}">締め <%= current_month_end %></td>
        </tr>
    </table>
    
    <%= link_to "勤怠を編集", login_path, class: "btn btn-default btn-lg active" %>

    
    
     <%= render "works/work_display_#{@month}" %>
 
    
    
    
  </div>
  
  